get-deep-orange(s = false)
  if s == 50
    rgba(#fbe9e7)
  else if s == 100
    rgba(#ffccbc)
  else if s == 200
    rgba(#ffab91)
  else if s == 300
    rgba(#ff8a65)
  else if s == 400
    rgba(#ff7043)
  else if s == 500
    rgba(#ff5722)
  else if s == 600
    rgba(#f4511e)
  else if s == 700
    rgba(#e64a19)
  else if s == 800
    rgba(#d84315)
  else if s == 900
    rgba(#bf360c)
  else if s == 1000
    rgba(#ff9e80)
  else if s == 1100
    rgba(#ff6e40)
  else if s == 1200
    rgba(#ff3d00)
  else if s == 1300
    rgba(#dd2c00)
  else 
    rgba(#ff5722)
fill-deep-orange(s = false)
  background-color:get-deep-orange(s)
text-deep-orange(s = false)
  color:get-deep-orange(s)
border-deep-orange(s = false)
  border-color:get-deep-orange(s)
.fill-deep-orange
  fill-deep-orange()
  .fill-minus-1,
  &.fill-minus-1
    fill-deep-orange(400)
  .fill-minus-2,
  &.fill-minus-2
    fill-deep-orange(300)
  .fill-minus-3,
  &.fill-minus-3
    fill-deep-orange(200)
  .fill-minus-4,
  &.fill-minus-4
    fill-deep-orange(100)
  .fill-minus-5,
  &.fill-minus-5
    fill-deep-orange(50)
  .fill-plus-1,
  &.fill-plus-1
    fill-deep-orange(600)
  .fill-plus-2,
  &.fill-plus-2
    fill-deep-orange(700)
  .fill-plus-3,
  &.fill-plus-3
    fill-deep-orange(800)
  .fill-plus-4,
  &.fill-plus-4
    fill-deep-orange(900)
  .fill-accent-1,
  &.fill-accent-1
    fill-deep-orange(1000)
  .fill-accent-2,
  &.fill-accent-2
    fill-deep-orange(1100)
  .fill-accent-3,
  &.fill-accent-3
    fill-deep-orange(1200)
  .fill-accent-4,
  &.fill-accent-4
    fill-deep-orange(1300)
  .fill-base,
  &.fill-base
    fill-deep-orange(500)
